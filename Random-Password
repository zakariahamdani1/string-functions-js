function randomPassword(param) {
  let bigStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*()";
  let max = bigStr.length;
  let randArray = [];
  let passwdArray = [];
  for(let i = 0; i < param; i++ ){
    randArray.push(Math.floor(Math.random() * max));
  }
  for(let x of randArray){
    passwdArray.push(bigStr[x]);
  }
  return passwdArray.join("")
}
